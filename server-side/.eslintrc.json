{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "no-debugger": "error",
        "no-console": "error",
        "eqeqeq": "error"
    }
}
